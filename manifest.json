{
  "manifest_version" : 3,
  "name" : "Tab Grouper",
  "version" : "1.0",
  "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },

  "action" : {
      "default_popup" : "popup.html"
  },

  "permissions": [
      "tabGroups",
      "tabs"
  ],

  "_execute_action" : {
    "suggested-key" : "Ctrl+G"
  },

  "description" : "Runs the extension on the page."
}