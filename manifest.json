{
  "manifest_version" : 3,
  "name" : "Tab Grouper",
  "version" : "1.0",
  "icons": {
      "16": "images/object-icon-16.png",
      "32": "images/object-icon-32.png",
      "48": "images/object-icon-48.png",
      "128": "images/object-icon-128.png"
    },

  "action" : {
      "default_popup" : "popup.html"
  },

  "permissions": [
      "tabGroups",
      "tabs"
  ],

  "commands" : {
    "_execute_action" : {
        "suggested_key" : {
          "default" : "Ctrl+G",
          "mac" : "Command+G"
        }
      }
    },
    "description" : "Runs the extension on the page."
}